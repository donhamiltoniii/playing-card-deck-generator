{
  "name": "playing-card-deck-generator",
  "version": "1.4.0",
  "description": "A simple library for generating playing card decks",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "prepublish": "yarn build",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "watch": "rollup -cw"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/donhamiltoniii/playing-card-deck-generator.git"
  },
  "keywords": [
    "playing",
    "cards",
    "generator"
  ],
  "author": "Don Hamilton III <donhamiltoniii@gmail.com> (https://donhamiltoniii.com)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/donhamiltoniii/playing-card-deck-generator/issues"
  },
  "homepage": "https://github.com/donhamiltoniii/playing-card-deck-generator#readme",
  "dependencies": {
    "@types/node": "^12.7.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "rollup": "^1.21.4",
    "rollup-plugin-typescript2": "^0.24.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    }
  }
}
